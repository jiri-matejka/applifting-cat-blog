{
  "name": "backend",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node build.ts",
    "build2": "tsc --build tsconfig.prod.json",
    "lint": "yarn eslint --ext .ts src/",
    "lint2": "yarn eslint **/*.{js,jsx,cjs,json,ts,tsx}",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start-prod": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "jest"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "bcrypt": "~5.1.0",
    "cookie-parser": "~1.4.6",
    "dotenv": "~16.3.1",
    "express": "~4.18.2",
    "express-async-errors": "~3.1.1",
    "helmet": "~7.0.0",
    "inserturlparams": "~1.0.1",
    "jet-logger": "~1.3.1",
    "jet-validator": "~1.1.1",
    "jsonfile": "~6.1.0",
    "jsonwebtoken": "~9.0.1",
    "module-alias": "~2.2.3",
    "morgan": "~1.10.0",
    "pg": "~8.11.2",
    "ts-command-line-args": "~2.5.1",
    "typeorm": "~0.3.17",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/cookie-parser": "~1.4.3",
    "@types/express": "~4.17.17",
    "@types/find": "~0.2.1",
    "@types/fs-extra": "~11.0.1",
    "@types/jest": "~29.5.3",
    "@types/jsonfile": "~6.1.1",
    "@types/jsonwebtoken": "~9.0.2",
    "@types/morgan": "~1.9.4",
    "@types/node": "~20.4.5",
    "@types/supertest": "~2.0.12",
    "@types/uuid": "~9.0.2",
    "@typescript-eslint/eslint-plugin": "~6.2.1",
    "@typescript-eslint/parser": "~6.2.1",
    "eslint": "~8.46.0",
    "eslint-plugin-node": "~11.1.0",
    "find": "~0.3.0",
    "fs-extra": "~11.1.1",
    "jest": "~29.6.2",
    "nodemon": "~3.0.1",
    "prettier": "~3.0.0",
    "supertest": "~6.3.3",
    "ts-jest": "~29.1.1",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.2.0",
    "typescript": "~5.1.6"
  }
}
